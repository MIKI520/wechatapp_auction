/* pages/me/me.wxss */
body{
    margin: 0px;
    padding: 0px;
}
.me_menu_1{
    background-color: #80ffaa;
}
.me_menu_1 .head .head_icon{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.me_menu_1 .head .head_icon text{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.me_menu_1 .head{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}
.me_menu_1 .head .head_text{
    line-height: 100px;
    margin-top: 10rpx;
}
.me_menu_1 .head image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-top: 30rpx;
    margin-right: 30rpx;
}